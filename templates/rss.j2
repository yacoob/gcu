<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <title>tactical grace</title>
    <link>{{ base_url }}</link>
    <description>Newest kits from GCU tactical grace.</description>
    <lastBuildDate>{{ last_update_date|datetime_to_rfc822 }}</lastBuildDate>
    <atom:link href="{{ base_url }}/index.xml" rel="self" type="application/rss+xml" />
{% for (post, title) in gcu['newest'][0:15] %}
<item>
{% set url = base_url + post.redir_url %}
  <title>{{ title }}</title>
  <link>{{ url }}</link>
  <pubDate>{{ post.meta.date|datetime_to_rfc822 }}</pubDate>
  <guid>{{ url }}</guid>
  <content:encoded><![CDATA[
    {{ post['content'] }}
  ]]></content:encoded>
</item>
{% endfor %}
  </channel>
</rss>
