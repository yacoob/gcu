{% import 'macros.j2' as macros %}
<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:content="http://purl.org/rss/1.0/modules/content/">
  <channel>
    <title>tactical grace</title>
    <link>{{ base_url }}</link>
    <description>Newest kits from GCU tactical grace.</description>
    <lastBuildDate>{{ last_site_update_date|datetime_to_rfc822 }}</lastBuildDate>
    <atom:link href="{{ base_url }}/index.xml" rel="self" type="application/rss+xml" />
{% for entry in gcu['newest_entries'][0:15] %}
<item>
{% set url = base_url + entry.canonical_url %}
{% set date = entry.date|short_date %}
  <title>{{ date }}</title>
  <link>{{ url }}</link>
  <pubDate>{{ entry.date|datetime_to_rfc822 }}</pubDate>
  <guid>{{ url }}</guid>
  <content:encoded><![CDATA[
    <h2>{{ date }}</h2>
    {{ macros.entry_photos(entry) }}
  ]]></content:encoded>
</item>
{% endfor %}
  </channel>
</rss>
